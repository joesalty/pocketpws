<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center_horizontal|top"
    android:orientation="vertical" >
    <TextView
            android:id="@+id/rtstation_location"
            style="@style/Text_big_roboto"
            android:text="@string/na" 
            android:paddingTop="10dp"/>
    <TextView
            android:id="@+id/rtstation_date"
            style="@style/Text_small_roboto"
            android:text="@string/na" 
            android:paddingTop="5dp"/>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="13dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <ImageView
                style="@style/smallimage"
                android:src="@drawable/rsunrise" />

            <TextView
                android:id="@+id/rtsunrise"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:layout_weight="0.33"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <ImageView
                style="@style/smallimage"
                android:src="@drawable/rsunset" />
            <TextView
                android:id="@+id/rtsunset"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:layout_weight="0.33"
                android:paddingLeft="1dp"
                android:text="@string/na" />
            <ImageView
                style="@style/smallimage"
                android:src="@drawable/rmoonrise" />
            <TextView
                android:id="@+id/rtmoonrise"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:layout_weight="0.33"
                android:text="@string/na" />
            <ImageView
                style="@style/smallimage"
                android:src="@drawable/rmmonset" />
            <TextView
                android:id="@+id/rtmoonset"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:text="@string/na" />
        </LinearLayout>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
            	
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rthermometer" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmax"
                android:paddingLeft="16dp" />
            </FrameLayout>
            <TextView
                android:id="@+id/rthigh_temp"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />
            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:text="@string/degree" />
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rthermometer" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmin" 
                android:paddingLeft="16dp"/>
            </FrameLayout>
            <TextView
                android:id="@+id/rtlow_temp"
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />
            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:text="@string/degree" />
     </LinearLayout>
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rheatindex" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmax" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rthigh_heatindex"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />
            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:text="@string/degree" />
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rwindchill" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmin" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtlow_windchill"
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />
            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:text="@string/degree" />
     </LinearLayout>
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rdew" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmax" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rthigh_dew"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />
            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:text="@string/degree" />
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rdew" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmin" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtlow_dew"
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />
            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:text="@string/degree" />
     </LinearLayout>
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rhumidity" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmax" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rthigh_hum"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:paddingLeft="2dp"
                android:text="%" />

            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rhumidity" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmin" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtlow_hum"
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <TextView
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="%" />

     </LinearLayout>
     <LinearLayout
