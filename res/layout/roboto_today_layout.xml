            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rpressure" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmax" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rthigh_barometer"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <TextView
                android:id="@+id/rtbarunit"
                style="@style/Text_small_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rpressure" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmin" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtlow_barometer"
                style="@style/Text_normal_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <TextView
                android:id="@+id/rtbarunit2"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />

     </LinearLayout>
     
      <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rwind" />
                <ImageView
                style="@style/normalimage"
                android:src="@drawable/rmax" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rthigh_windspeed"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <TextView
                android:id="@+id/rtwindunit"
                style="@style/Text_small_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <ImageView
                style="@style/normalimage"
                android:src="@drawable/rrain" />
            <TextView
                android:id="@+id/rttodaysrain"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"
                android:text="@string/na" />

            <TextView
                android:id="@+id/rtrainunit"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"
                android:text="@string/na" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:id="@+id/rtcustom1_pic" />
                <ImageView
                android:id="@+id/rtcustom1_pic_max"
                style="@style/normalimage" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtcustom1_data_max"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"/>

            <TextView
                android:id="@+id/rtcustom1_unit"
                style="@style/Text_small_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:paddingLeft="2dp"/>

            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:id="@+id/rtcustom1_pic2" />
                <ImageView
                android:id="@+id/rtcustom1_pic_min"
                style="@style/normalimage" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtcustom1_data_min"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"/>

            <TextView
                android:id="@+id/rtcustom1_unit2"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp" >
            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:id="@+id/rtcustom2_pic" />
                <ImageView
                android:id="@+id/rtcustom2_pic_max"
                style="@style/normalimage"
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtcustom2_data_max"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"/>

            <TextView
                android:id="@+id/rtcustom2_unit"
                style="@style/Text_small_roboto"
                android:layout_width="0dp"
                android:layout_weight="1.00"
                android:paddingLeft="2dp"/>

            <FrameLayout 
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content">
                
                <ImageView
                style="@style/normalimage"
                android:id="@+id/rtcustom2_pic2" />
                <ImageView
                android:id="@+id/rtcustom2_pic_min"
                style="@style/normalimage" 
                android:paddingLeft="16dp"/>
                
            </FrameLayout>
            <TextView
                android:id="@+id/rtcustom2_data_min"
                style="@style/Text_normal_roboto"
                android:paddingLeft="2dp"/>

            <TextView
                android:id="@+id/rtcustom2_unit2"
                style="@style/Text_small_roboto"
                android:layout_width="wrap_content"
                android:paddingLeft="2dp"/>

        </LinearLayout>
        

</LinearLayout>